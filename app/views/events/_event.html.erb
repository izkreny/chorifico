<div id="<%= dom_id event %>">
  <div>
    <strong>Name:</strong>
    <%= event.name %>
  </div>

  <div>
    <strong>Status:</strong>
    <%= event.status %>
  </div>

  <div>
    <strong>Type:</strong>
    <%= event.type %>
  </div>

  <div>
    <strong>Start:</strong>
    <%= event.start %>
  </div>

  <div>
    <strong>End:</strong>
    <%= event.end %>
  </div>

  <div>
    <strong>Location:</strong>
    <%= event.location %>
  </div>

  <div>
    <strong>Description:</strong>
    <%= event.description %>
  </div>

</div>
